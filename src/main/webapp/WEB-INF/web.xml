<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchPageServlet</servlet-name>
  	<display-name>SearchPageServlet</display-name>
  	<description></description>
  	<servlet-class>SearchPageServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>ResultsPageServlet</servlet-name>
	<servlet-class>ResultsPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RecipeDetailsPageServlet</servlet-name>
    <display-name>RecipeDetailsPageServlet</display-name>
    <description></description>
    <servlet-class>RecipeDetailsPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RestaurantDetailsPageServlet</servlet-name>
    <display-name>RestaurantDetailsPageServlet</display-name>
    <description></description>
    <servlet-class>RestaurantDetailsPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListManagementPageServlet</servlet-name>
    <display-name>ListManagementPageServlet</display-name>
    <description></description>
    <servlet-class>ListManagementPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchPageServlet</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ResultsPageServlet</servlet-name>
  	<url-pattern>/results</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RecipeDetailsPageServlet</servlet-name>
    <url-pattern>/recipeDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RestaurantDetailsPageServlet</servlet-name>
    <url-pattern>/restaurantDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListManagementPageServlet</servlet-name>
    <url-pattern>/listManagement</url-pattern>
  </servlet-mapping>
</web-app>
