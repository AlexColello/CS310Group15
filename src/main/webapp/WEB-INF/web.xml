<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchPageServlet</servlet-name>
  	<display-name>SearchPageServlet</display-name>
  	<description></description>
  	<servlet-class>SearchPageServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>ResultsPageServlet</servlet-name>
	<servlet-class>ResultsPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchPageServlet</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ResultsPageServlet</servlet-name>
  	<url-pattern>/results</url-pattern>
  </servlet-mapping>
</web-app>
